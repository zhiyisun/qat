			mkdir /lib/firmware/qat_fw_backup;	\
		fi;	\
 		$(foreach bin, $(notdir $(BIN_LIST)),							\
			if [ -e /lib/firmware/$(bin) ]; then						\
				mv /lib/firmware/$(bin) /lib/firmware/qat_fw_backup/$(bin);		\
			fi;										\
			if [ -e $(ICP_BUILD_OUTPUT)/$(bin) ]; then					\
				$(INSTALL) -D -m 750 $(ICP_BUILD_OUTPUT)/$(bin) /lib/firmware/$(bin);	\
			fi;										\
		)	\
		if [ ! -d /etc/qat_conf_backup ]; then							\
			$(MKDIR) /etc/qat_conf_backup;							\
		fi;	\
		$(MV) /etc/c6xx*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
		$(MV) /etc/d15xx*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
		$(MV) /etc/c3xxx*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
		$(MV) /etc/c4xxx*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
		$(MV) /etc/4xxx*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
		$(MV) /etc/vqat*.conf /etc/qat_conf_backup/ 2>/dev/null || :;	\
														\
		if [ `lsmod | grep "qat_4xxx" | wc -l` != "0" ]; then                                           \
                        rmmod qat_4xxx;                                                                         \
                fi;                                                                                             \
                if [ `lsmod | grep "intel_qat" | wc -l` != "0" ]; then                                          \
                        rmmod intel_qat;                                                                        \
                fi;                                                                                             \
														\
		platform="SOC";											\
		sel_dc="DC_DEFAULT";										\
		sel_sym="SYM_DEFAULT";										\
														\
		if [ "$(ICP_DC_ONLY)" == "1" ]; then								\
			sel_dc="DC_CONF";									\
		fi;												\
														\
		if [ "$(ICP_DC_SYM_ONLY_AM)" == "1" ]; then							\
			sel_dc="DC_CONF";									\
			sel_sym="SYM_CONF";									\
		fi;												\
														\
		if [ "$(ICP_SRIOV_AM)" != "2" ]; then										\
			if [ "$(num4xxxDevicesP)" -gt "0" ]; then									\
				$(ICP_ROOT)/generate_conf_files.sh -n $(num4xxxDevicesP) 					\
					-f $(ICP_BUILD_OUTPUT)/4xxx_template.conf -o $(ICP_BUILD_OUTPUT);			\
			fi;													\
			for (( dev=0; dev<$(num4xxxDevicesP); dev++ ))								\
			do													\
				$(INSTALL) -D -m 640 $(ICP_BUILD_OUTPUT)/4xxx_dev$$dev.conf /etc/4xxx_dev$$dev.conf;		\
			done;													\
		fi;	\
		if [ "$(ICP_SRIOV_AM)" == "1" ]; then												\
			for (( dev=0; dev<$(num4xxxDevicesP); dev++ ))										\
			do															\
				for (( vf_dev = 0; vf_dev<$(QAT_4XXX_NUM_VFS); vf_dev++ ))							\
				do														\
					vf_dev_num=$$(($$dev * $(QAT_4XXX_NUM_VFS) + $$vf_dev)); 						\
					$(INSTALL) -D -m 640 $(ICP_BUILD_OUTPUT)/4xxxvf_dev0.conf.vm /etc/4xxxvf_dev$$vf_dev_num.conf;		\
				done;														\
			done;															\
		else																\
			for (( dev=0; dev<$(num4xxxDevicesV); dev++ ))										\
			do															\
				$(INSTALL) -D -m 640 $(ICP_BUILD_OUTPUT)/4xxxvf_dev0.conf.vm /etc/4xxxvf_dev$$dev.conf;				\
			done;                                                                                                                   \
			if [ $(numVQATDevices) != 0 ];then	\
				$(ICP_ROOT)/config_vqat.sh; 	\
			fi;	\
		fi;	\
		$(ECHO) "Creating startup and kill scripts";	\
																								\
		$(INSTALL) -D -m 750 $(ICP_BUILD_OUTPUT)/qat_service $(QAT_SERVICE_PATH)/qat_service;	\
																								\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == 2 ]; then			\
			echo "# Remove comment on next line to enable sriov" > /etc/default/qat;	\
			echo "SRIOV_ENABLE=1" >> /etc/default/qat;					\
		else											\
			echo "# Comment or remove next line to disable sriov" > /etc/default/qat;	\
			echo "#SRIOV_ENABLE=1" >> /etc/default/qat;					\
		fi;	\
		$(ECHO) "Copying libqat_s.so to $(libdir)";	\
		$(INSTALL) -D -m 755 $(ICP_BUILD_OUTPUT)/libqat_s.so $(libdir)/libqat_s.so;	\
		$(ECHO) "Copying libusdm_drv_s.so to $(libdir)";	\
		$(INSTALL) -D -m 755 $(ICP_BUILD_OUTPUT)/libusdm_drv_s.so $(libdir)/libusdm_drv_s.so;	\
		$(ECHO) $(libdir) > /etc/ld.so.conf.d/qat.conf; ldconfig;   \
		$(ECHO) "Copying usdm module to system drivers";	\
	       	if [ ! -z '$(mod_sign_cmd)' ]; then \
               		$(ECHO) "Need to sign driver $(ICP_BUILD_OUTPUT)/usdm_drv.ko."; \
		        $(mod_sign_cmd) $(ICP_BUILD_OUTPUT)/usdm_drv.ko; \
		fi;	\
		$(CP) -rf  $(ICP_BUILD_OUTPUT)/usdm_drv.ko  "/lib/modules/`\uname -r`/kernel/drivers";	\
		if [ ! `getent group qat` ]; then $(ECHO) "Creating qat group."; groupadd qat; else $(ECHO) "Group qat already exists."; fi;	\
		$(ECHO) "Creating udev rules";	\
		if [ ! -e /etc/udev/rules.d/00-qat.rules ]; then							\
			echo 'KERNEL=="qat_adf_ctl" MODE="0660" GROUP="qat"' > /etc/udev/rules.d/00-qat.rules;		\
			echo 'KERNEL=="qat_dev_processes" MODE="0660" GROUP="qat"' >> /etc/udev/rules.d/00-qat.rules;	\
			echo 'KERNEL=="usdm_drv" MODE="0660" GROUP="qat"' >> /etc/udev/rules.d/00-qat.rules;		\
			echo 'ACTION=="add", DEVPATH=="/module/usdm_drv" SUBSYSTEM=="module" RUN+="/bin/mkdir $(HUGE_PAGE_DIR)"' >> /etc/udev/rules.d/00-qat.rules;  \
			echo 'ACTION=="add", DEVPATH=="/module/usdm_drv" SUBSYSTEM=="module" RUN+="/bin/chgrp qat $(HUGE_PAGE_DIR)"' >> /etc/udev/rules.d/00-qat.rules;  \
			echo 'ACTION=="add", DEVPATH=="/module/usdm_drv" SUBSYSTEM=="module" RUN+="/bin/chmod 0770 $(HUGE_PAGE_DIR)"' >> /etc/udev/rules.d/00-qat.rules;  \
			echo 'ACTION=="remove", DEVPATH=="/module/usdm_drv" SUBSYSTEM=="module" RUN+="/bin/rmdir $(HUGE_PAGE_DIR)"' >> /etc/udev/rules.d/00-qat.rules;  \
			$(foreach dev, $(DEVICES), echo 'KERNEL=="uio*", ATTRS{vendor}=="0x'`echo $(INTEL_VENDORID) | tr -d \"`'", ATTRS{device}=="0x'`echo $(dev) | tr -d \"`'"  MODE="0660" GROUP="qat"' >> /etc/udev/rules.d/00-qat.rules; ) \
		fi;	\
		$(ECHO) "Creating module.dep file for QAT released kernel object";	\
		$(ECHO) "This will take a few moments";	\
		depmod -a;	\
		if [ `lsmod | grep "usdm_drv" | wc -l` != "0" ]; then	\
			rmmod usdm_drv;					\
		fi;	\
		if [ -e /usr/bin/systemctl ]; then	\
				if [ "$(SYSTEMD_SYSTEM_UNIT_DIR)" == "" ]; then	\
					$(ECHO) "Could not find path to place systemd unit file. Using /usr/lib/systemd/system by default";	\
				fi;	\
				$(CP) -f $(SYSTEMD_UNIT_FILE_PATH)/$(SYSTEMD_UNIT_FILE) $(SYSTEMD_SYSTEM_UNIT_DIR);	\
				systemctl enable qat;	\
				systemctl daemon-reload;	\
		elif [ -e /sbin/chkconfig ] ; then			\
				chkconfig --add qat_service;			\
		elif [ -e /usr/sbin/update-rc.d ]; then			\
				update-rc.d qat_service defaults;               \
		fi;	\
		$(ECHO) "Starting QAT service";	\
			$(QAT_SERVICE_PATH)/qat_service shutdown;       \
		$(SLEEP) 3;	\
			$(QAT_SERVICE_PATH)/qat_service start;       \
		if [ `lsmod |egrep -c "usdm_drv"` == "0" ]; then						\
			$(ECHO) -e "\n\t*** Error:usdm_drv module not installed ***\n";				\
		fi;												\
		if [ $(num4xxxDevicesP) != 0 ];then								\
			if [ `lsmod |egrep -c "qat_4xxx"` == "0" ]; then					\
				$(ECHO) -e "\n\t*** Error:qat_4xxx module not installed ***\n";		\
			fi;											\
			if [ "$(ICP_SRIOV_AM)" == 1 ]; then							\
				if [ `lsmod |egrep -c "qat_4xxxvf"` == "0" ]; then				\
					$(ECHO) -e "\n\t*** Error:qat_4xxxvf module not installed ***\n";	\
				fi;										\
			fi;											\
		fi;												\
		if [ "$(ICP_SRIOV_AM)" == "2" ]; then								\
			if [ $(num4xxxDevicesV) != 0 ];then							\
				if [ `lsmod |egrep -c "qat_4xxxvf"` == "0" ]; then				\
					$(ECHO) -e "\n\t*** Error:qat_4xxxvf module not installed ***\n";	\
				fi;										\
			fi;											\
			if [ $(numVQATDevices) != 0 ];then							\
				if [ `lsmod |egrep -c "qat_vqat"` == "0" ]; then				\
					$(ECHO) -e "\n\t*** Error:qat_vqat module not installed ***\n";		\
				fi;										\
			fi;											\
		fi;												\
		if [ `$(ICP_BUILD_OUTPUT)/adf_ctl status | grep -c "state: down"` != "0" ]; then		\
			$(ECHO) -e "\n\t*** Error:QAT driver not activated ***\n";				\
		fi

qat-service-uninstall:
	@if [ $(shell lsmod | grep "qat" | wc -l) != "0" -o -e						\
		$(KERNEL_SOURCE_ROOT_UPDATE)/drivers/crypto/qat/qat_common/intel_qat.ko ]; then		\
		$(ECHO) "Removing the QAT firmware";							\
		$(foreach bin, $(notdir $(BIN_LIST)),							\
			if [ -e /lib/firmware/$(bin) ]; then						\
				$(RM) /lib/firmware/$(bin);						\
			fi;										\
			if [ -e /lib/firmware/qat_fw_backup/$(bin) ]; then				\
				$(MV) /lib/firmware/qat_fw_backup/$(bin) /lib/firmware/$(bin);		\
			fi;										\
		)											\
													\
		if [ -d /lib/firmware/qat_fw ]; then							\
			$(RM) /lib/firmware/qat_fw_backup;						\
		fi;											\
													\
		if [ `lsmod | grep "usdm_drv" | wc -l` != "0" ]; then					\
			rmmod usdm_drv;									\
		fi; 											\
													\
		if [ -e /etc/init.d/qat_service_upstream ]; then \
			until /etc/init.d/qat_service_upstream shutdown; \
			do \
				sleep 1; \
			done; \
			if [ -e /sbin/chkconfig ]; then \
				chkconfig --del qat_service_upstream; \
			elif [ -e /usr/sbin/update-rc.d ]; then \
				update-rc.d -f qat_service_upstream remove; \
			fi; \
			$(RM) /etc/init.d/qat_service_upstream; \
		elif [ -e $(QAT_SERVICE_PATH)/qat_service ]; then \
			until $(QAT_SERVICE_PATH)/qat_service shutdown; \
			do \
				sleep 1; \
			done; \
			if [ -e /usr/bin/systemctl ]; then \
				systemctl stop qat; \
				systemctl disable qat; \
				$(RM) -f $(SYSTEMD_SYSTEM_UNIT_DIR)/$(SYSTEMD_UNIT_FILE); \
			elif [ -e /sbin/chkconfig ]; then \
				chkconfig --del qat_service; \
			elif [ -e /usr/sbin/update-rc.d ]; then \
				update-rc.d -f qat_service remove; \
			fi; \
			$(RM) $(QAT_SERVICE_PATH)/qat_service; \
		fi; \
		if [ -e /usr/bin/systemctl ]; then									\
			systemctl daemon-reload;										\
		fi;																	\
													\
		$(RM) $(libdir)/libqat_s.so;								\
		$(RM) $(libdir)/libusdm_drv_s.so;                                                       \
		$(RM) -rf /etc/ld.so.conf.d/qat.conf; ldconfig;                                         \
											                \
		$(ECHO) "Removing config files";							\
		$(RM) /etc/c6xx*.conf;									\
		$(RM) /etc/c3xxx*.conf;									\
		$(RM) /etc/d15xx*.conf;									\
		$(RM) /etc/c4xxx*.conf;									\
		$(RM) /etc/4xxx*.conf;									\
		$(RM) /etc/vqat*.conf;									\
		$(RM) /etc/default/qat;									\
		$(RM) /etc/udev/rules.d/00-qat.rules;							\
													\
		@mv -f /etc/qat_conf_backup/dh895xcc*.conf /etc/ 2>/dev/null || :;			\
		@mv -f /etc/qat_conf_backup/c6xx*.conf /etc/ 2>/dev/null || :;				\
		@mv -f /etc/qat_conf_backup/c3xxx*.conf /etc/ 2>/dev/null || :;				\
		@mv -f /etc/qat_conf_backup/d15xx*.conf /etc/ 2>/dev/null || :;				\
		@mv -f /etc/qat_conf_backup/c4xxx*.conf /etc/ 2>/dev/null || :;				\
		@mv -f /etc/qat_conf_backup/4xxx*.conf /etc/ 2>/dev/null || :;				\
		@mv -f /etc/qat_conf_backup/vqat*.conf /etc/ 2>/dev/null || :;				\
													\
		$(ECHO) "Removing drivers modules";							\
		$(RM) $(KERNEL_SOURCE_ROOT_UPDATE)/drivers/crypto/qat;					\
                $(RM)   "/lib/modules/`\uname -r`/kernel/drivers"/usdm_drv.ko		        	\
		$(ECHO) "Creating module.dep file for QAT released kernel object";			\
		depmod -a;										\
													\
		if [ `lsmod |egrep -c "usdm_drv|intel_qat"` != "0" ]; then				\
			if [ `modinfo intel_qat |egrep -c "updates"` == "0" ]; then			\
				$(ECHO) -e "\n\t***  In-tree driver loaded ***\n";			\
				$(ECHO) -e "\n\n\n\n\t***************************************";		\
				$(ECHO) -e "\n\t*** Acceleration Uninstall Complete ***";		\
				$(ECHO) -e "\n\t***************************************\n\n\n\n\n";	\
			else										\
				$(ECHO) -e "\n\t*** Error:Some modules not Removed Properly***\n";	\
				$(ECHO) -e "\n\n\n\n\t***************************************";		\
				$(ECHO) -e "\n\t*** Acceleration Uninstall Failed ***";			\
				$(ECHO) -e "\n\t***************************************\n\n\n\n\n";	\
			fi;										\
		else											\
			$(ECHO) -e "\n\n\n\n\t***************************************";			\
			$(ECHO) -e "\n\t*** Acceleration Uninstall Complete ***";			\
			$(ECHO) -e "\n\t***************************************\n\n\n\n\n";		\
		fi;											\
		if [ $(num4xxxDevicesP) != 0 ];then                                                            \
			lsmod | grep qat_4xxx >/dev/null 2>&1 || modprobe -b -q qat_4xxx >/dev/null 2>&1 || true;     \
		fi;												\
		if [ $(num4xxxDevicesV) != 0 ];then								\
			lsmod | grep qat_4xxxvf >/dev/null 2>&1 || modprobe -b -q qat_4xxxvf >/dev/null 2>&1 || true;	\
		fi;												\
		if [ $(numVQATDevices) != 0 ];then								\
			lsmod | grep qat_vqat >/dev/null 2>&1 || modprobe -b -q qat_vqat >/dev/null 2>&1 || true;	\
		fi;												\
	else												\
		$(ECHO) -e "\n\n\t*** Acceleration Package Not Installed ***>\n\n\n";			\
	fi

qat-service-clean:
	@$(ECHO) qat-service-clean

